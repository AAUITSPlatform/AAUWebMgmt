@model List<ITSWebMgmt.Models.TableModel>
<h2>Purchase info</h2>
@if (Model.Count > 1)
{
    <h2>Multiple results found! All are listed below</h2>
    @foreach (var table in Model)
    {
        @Html.Partial("TableView", table)
        <br />
    }
}
else
{
    @Html.Partial("TableView", Model[0])
}